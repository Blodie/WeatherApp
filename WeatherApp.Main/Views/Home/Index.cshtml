@model List<WeatherViewModel>

@{
    ViewData["Title"] = "Home Page";
}

<h1>Current Weather</h1>
<hr />

<table class="table">
    <tr>
        <th>Country</th>
        <th>City</th>
        <th>Temperature (C)</th>
    </tr>
    @foreach (var weatherVM in Model)
    {
        <tr>
            <td>weatherVM.Country</td>
            <td>weatherVM.City</td>
            <td>weatherVM.CurrentWeather.TempC</td>
        </tr>
    }
</table>